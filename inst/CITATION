citHeader("To cite eAnalytics in publications use:")

bibentry(
  bibtype    = "Article",
  title    = "eAnalytics: Dynamic Web-based Analytics for the Energy Industry",
  author   = "Paul Govan",
  journal  = "Journal of Open Research Software",
  year     = "2016",
  volume   = "4",
  number   = "1",
  pages    = "e45",
  url      = "https://openresearchsoftware.metajnl.com/articles/10.5334/jors.144",
  doi      = "10.5334/jors.144"
)

bibentry(
  bibtype = "Manual",
  title = "eAnalytics: Dynamic Web-based Analytics for the Energy Industry",
  author = "Paul Govan",
  year = "2023",
  note = "R package version 0.3",
  url = "https://CRAN.R-project.org/package=eAnalytics",
  doi = "10.32614/CRAN.package.eAnalytics"
)

